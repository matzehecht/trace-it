{
  "name": "trace-it",
  "version": "0.0.0-development",
  "description": "This package provides a lightweight performance tracing/measuring tool based on transaction trees. (Inspired by the performance monitoring from sentry.io. But is not distributed)",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "build": "tsc",
    "semantic-release": "semantic-release"
  },
  "bin": "bin/cli",
  "author": {
    "name": "Matthias Hecht",
    "email": "matthias@hecht.space",
    "url": "https://hecht.space"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/lowdb": "^1.0.9",
    "@types/node": "^14.11.1",
    "semantic-release": "^17.1.2",
    "shx": "^0.3.2",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "lowdb": "^1.0.0",
    "nanoid": "^3.1.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matzehecht/trace-it.git"
  },
  "keywords": [
    "performace",
    "transaction",
    "measuring",
    "tracing",
    "trace"
  ],
  "bugs": {
    "url": "https://github.com/matzehecht/trace-it/issues"
  },
  "homepage": "https://github.com/matzehecht/trace-it",
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      {
        "name": "dev",
        "prerelease": true
      }
    ]
  }
}
